{
  "name": "gapi-cli",
  "version": "0.2.9",
  "description": "Gapi command line interface",
  "main": "main.js",
  "scripts": {
    "postinstall": "cp -r ./bash/lib/* ./node_modules/@gql2ts/",
    "generate:schema": "apollo-codegen introspect-schema http://localhost:9000/graphql --output ./api-types/schema.json",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@gitlab.youvolio.com:522/open-source/gapi-cli.git"
  },
  "keywords": [
    "Gapi",
    "CLI",
    "graphql"
  ],
  "bin": {
    "gapi": "root.js"
  },
  "author": "Kristiyan Tachev (@Stradivario)",
  "license": "MIT",
  "peerDependencies": {
    "nodemon": "1.17.1",
    "ts-node": "5.0.1"
  },
  "dependencies": {
    "apollo-codegen": "^0.19.0",
    "gql2ts": "^1.7.0",
    "rxjs": "^5.5.6",
    "shelljs": "^0.8.1",
    "typedi": "^0.7.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/shelljs": "^0.7.8",
    "reflect-metadata": "^0.1.12",
    "ts-lint": "^4.5.1",
    "ts-node": "5.0.1",
    "typescript": "^2.7.1"
  }
}
